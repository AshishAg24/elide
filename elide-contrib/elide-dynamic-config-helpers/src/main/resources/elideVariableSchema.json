{
    "$schema": "http://json-schema.org/schema#",
    "$id": "http://elide.io/schemas/variable_schema_v1#",
    "description": "Elide Variable config json/hjson schema",
    "type": "object",
    "patternProperties": {
        "^([A-Za-z]*_?[A-Za-z]*)*$": {
          "anyOf": [
            {"type": "string"},
            {"type": "array"},
            {"type": "null"}
          ]
        }
      },
      "additionalProperties": false,
      "minProperties": 1
}
